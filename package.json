{
  "name": "vue-memory-game",
  "version": "1.0.0",
  "description": "A tiny game written in vue. It's inspired by IgorMinar's Memory-Game",
  "scripts": {
    "start": "webpack-dev-server --content-base build/ --hot --inline",
    "dist": "rm -rf dist; webpack --config webpack.config.prod.js",
    "deploy": "npm run dist; git checkout gh-pages; rm -rf *.png; rm -rf *.js; rm -rf *.ico; rm -rf index.html; mv dist/* .;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leftstick/vue-memory-game.git"
  },
  "keywords": [
    "memory-game",
    "vue",
    "component"
  ],
  "author": "Howard.Zuo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leftstick/vue-memory-game/issues"
  },
  "homepage": "https://github.com/leftstick/vue-memory-game#readme",
  "dependencies": {
    "vue": "^1.0.24",
    "vuex": "^0.6.3",
    "wilddog": "^0.6.4"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.4.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
